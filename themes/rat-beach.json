			},
			{
				"scope": "support",
				"settings": {
					"foreground": "#005cc5"
				}
			},
			{
				"scope": "meta.property-name",
				"settings": {
					"foreground": "#005cc5"
				}
			},
			{
				"scope": "variable",
				"settings": {
